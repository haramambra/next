{
  "overrides": [
    {
      "files": "*.{js,jsx,ts,tsx}",
      "options": {
        "parser": "typescript",
        "useTabs": true,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": false,
        "printWidth": 120
      }
    },
    {
      "files": "*.css",
      "options": {
        "useTabs": true,
        "tabWidth": 4,
        "printWidth": 120
      }
    }
  ]
}
